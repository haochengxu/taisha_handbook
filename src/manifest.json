{
    "manifest_version": 2,

    "name": "taiha_handbook",
    "description": "this extension will randomly show taisha practice every day",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default": "太傻练习册"
    },
    "permissions": [
        "activeTab"
    ]
}